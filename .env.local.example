# Supabase
VITE_SUPABASE_URL=https://seu-projeto.supabase.co
VITE_SUPABASE_ANON_KEY=sua-chave-anonima

# Backend - Supabase Service Role (NUNCA exponha no frontend)
SUPABASE_SERVICE_ROLE_KEY=sua-service-role-key

# Efí PIX - Credenciais
EFI_CLIENT_ID=seu-client-id
EFI_CLIENT_SECRET=seu-client-secret
EFI_PIX_KEY=sua-chave-pix-uuid

# Efí PIX - Certificados (em Base64)
EFI_CERT_PEM_BASE64=seu-certificado-em-base64
EFI_KEY_PEM_BASE64=sua-chave-privada-em-base64

# Ambiente
EFI_ENV=production
